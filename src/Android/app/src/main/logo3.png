<?xml version="1.0" encoding="UTF-8"?>
<grammar ns="" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools" xmlns="http://relaxng.org/ns/structure/1.0" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
    <start>
        <element name="manifest">
            <element name="uses-feature">
                <attribute name="android:name">
                    <data type="NCName"/>
                </attribute>
            </element>
            <oneOrMore>
                <element name="uses-permission">
                    <optional>
                        <attribute name="android:maxSdkVersion">
                            <data type="integer"/>
                        </attribute>
                    </optional>
                    <attribute name="android:name">
                        <data type="NCName"/>
                    </attribute>
                    <optional>
                        <attribute name="tools:ignore">
                            <data type="NCName"/>
                        </attribute>
                    </optional>
                </element>
            </oneOrMore>
            <element name="application">
                <attribute name="android:allowBackup">
                    <data type="boolean"/>
                </attribute>
                <attribute name="android:dataExtractionRules"/>
                <attribute name="android:fullBackupContent"/>
                <attribute name="android:icon"/>
                <attribute name="android:label"/>
                <attribute name="android:name">
                    <data type="NMTOKEN"/>
                </attribute>
                <attribute name="android:roundIcon"/>
                <attribute name="android:supportsRtl">
                    <data type="boolean"/>
                </attribute>
                <attribute name="android:theme"/>
                <attribute name="tools:targetApi">
                    <data type="integer"/>
                </attribute>
                <oneOrMore>
                    <element name="activity">
                        <optional>
                            <attribute name="android:exported">
                                <data type="boolean"/>
                            </attribute>
                        </optional>
                        <optional>
                            <attribute name="android:label"/>
                        </optional>
                        <attribute name="android:name">
                            <data type="NMTOKEN"/>
                        </attribute>
                        <optional>
                            <attribute name="android:theme"/>
                        </optional>
                        <optional>
                            <element name="intent-filter">
                                <element name="action">
                                    <attribute name="android:name">
                                        <data type="NCName"/>
                                    </attribute>
                                </element>
                                <element name="category">
                                    <attribute name="android:name"/>
                                </element>
                            </element>
                        </optional>
                    </element>
                </oneOrMore>
                <element name="provider">
                    <attribute name="android:authorities"/>
                    <attribute name="android:exported">
                        <data type="boolean"/>
                    </attribute>
                    <attribute name="android:grantUriPermissions">
                        <data type="boolean"/>
                    </attribute>
                    <attribute name="android:name"/>
                    <element name="meta-data">
                        <attribute name="android:name"/>
                        <attribute name="android:resource"/>
                    </element>
                </element>
            </element>
        </element>
    </start>
</grammar>
